    "laravel-mix-bundle-analyzer": "^1.0.5"
  },
  "dependencies": {
    "@babel/core": "^7.8.4",
    "@babel/polyfill": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "axios": "^0.19.2",
    "babel-loader": "^8.0.6",
    "cross-env": "^7.0.0",
    "css-loader": "^3.4.2",
    "install-changed": "^1.1.0",
    "laravel-mix": "^5.0.1",
    "node-sass": "^4.13.1",
    "resolve-url-loader": "^3.1.1",
    "sass": "^1.25.0",
    "sass-loader": "^8.0.2",
    "swiper": "^5.3.6",
    "vue": "^2.6.11",
    "vue-template-compiler": "^2.6.11"
